{
   "author": "Bitter Buffalo",
   "browser_action": {
      "default_icon": "images/icon_16.png"
   },
   "content_scripts": [ {
      "all_frames": true,
      "css": [ "styles/style.css" ],
      "js": [ "scripts/script.js" ],
      "matches": [ "https://www.funimation.com/shows/*", "http://www.funimation.com/shows/*", "https://www.funimation.com/player/*", "http://www.funimation.com/player/*" ]
   } ],
   "description": "This extension modifies the Funation.com video player so it fits the width of the content section better.",
   "homepage_url": "http://www.myheartsabitterbuffalo.com/FuniFix",
   "icons": {
      "128": "images/icon_128.png",
      "16": "images/icon_16.png",
      "32": "images/icon_32.png",
      "48": "images/icon_48.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "FunimationFix",
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "0.2.1",
   "web_accessible_resources": [ "scripts/inject.js", "scripts/inject2.js" ]
}
